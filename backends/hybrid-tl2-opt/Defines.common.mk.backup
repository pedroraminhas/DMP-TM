CC       := gcc
CFLAGS   += -std=c++11 -g -w -pthread -mcpu=power8 -mtune=power8 -fpermissive
CFLAGS   += -O2
CFLAGS   += -I$(LIB) -I ../../../stms/tl2/ -L../../../stms/tl2 -ltl2
